@using EMEHospitalWebApp.Pages.Extensions
@model PatientsPage

<thead>
	@Html.MyEditorForLabel(m => m.Item.FullName)
	@Html.MyEditorForLabel(m => m.Item.FirstName)
	@Html.MyEditorForLabel(m => m.Item.LastName)
	@Html.MyEditorForLabel(m => m.Item.Gender)
	@Html.MyEditorForLabel(m => m.Item.BirthDate)
	@Html.MyEditorForLabel(m => m.Item.IdCode)
	<th></th>
</thead>
<tbody>
	@foreach (var item in Model.Items) {
		@Html.MyEditorForTable(modelItem => item.FullName)
		@Html.MyEditorForTable(modelItem => item.FirstName)
		@Html.MyEditorForTable(modelItem => item.LastName)
		@Html.MyEditorForTable(modelItem => item.Gender)
		@Html.MyEditorForTable(modelItem => item.BirthDate)
		@Html.MyEditorForTable(modelItem => item.IdCode, item.Id)
	}
</tbody>
